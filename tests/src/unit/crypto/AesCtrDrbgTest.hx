	private function aesCtrDrbgBlock(variant:AesVariant, entr:String, pers:String, resd:String, aadDrbg:String, aad1Drbg:String, aad2Drbg:String, exp:String,
			derv:Int):Void {
		var entropy = Bytes.ofHex(entr);
		var personalization = Bytes.ofHex(pers);
		var reseedEntropy = Bytes.ofHex(resd);
		var aad = Bytes.ofHex(aadDrbg);
		var aad1 = Bytes.ofHex(aad1Drbg);
		var aad2 = Bytes.ofHex(aad2Drbg);
		var expected = exp;
		var derivation:Bool = (derv == 1);
		var drbg = new AesCtrDrbg(variant, derivation);
		drbg.init(entropy, personalization);
		drbg.reseed(reseedEntropy, aad);
		drbg.generate(64, aad1); // omit
		var output = drbg.generate(64, aad2);
		eq(output.toHex().toUpperCase(), expected.toUpperCase());
	}

